<?xml version="1.0" encoding="utf-8"?>
<!--Page layout for floor 5-->
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/floor5">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <!--Button for fountain-->
        <Button
            android:layout_width="25dp"
            android:layout_height="30dp"
            android:id="@+id/f_5_1"
            android:clickable="true"
            android:alpha="0"
            android:layout_above="@+id/f_5_2"
            android:layout_alignEnd="@+id/f_5_2"
            android:layout_marginEnd="12dp"
            android:layout_marginBottom="10dp" />

        <!--Button for women's bathroom-->
        <Button
            android:layout_width="25dp"
            android:layout_height="50dp"
            android:id="@+id/f_5_2"
            android:clickable="true"
            android:alpha="0"
            android:layout_marginStart="29dp"
            android:layout_alignTop="@+id/f_5_3"
            android:layout_alignStart="@+id/f_5_3" />

        <!--Button for men's bathroom-->
        <Button
            android:layout_width="25dp"
            android:layout_height="50dp"
            android:id="@+id/f_5_3"
            android:clickable="true"
            android:alpha="0"
            android:layout_marginEnd="131dp"
            android:layout_marginBottom="149dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true" />

    </RelativeLayout>


</FrameLayout>